  "devDependencies": {
    "svgo": "^1.3.2"
  },
  "scripts": {
    "build": "hugo",
    "start": "hugo server -D --debug --disableFastRender --buildFuture",
    "logo": "svgo -i Source\\ Files/Logo/Blaufußtölpel.svg -o static/images/blaufusstoelpel.svg",
    "header": "svgo --config '{ \"plugins\": [ {\"removeAttrs\": { \"attrs\": \"(style|font-weight|font-size|font-family)\" } } ] }' --enable=removeAttrs -i Source\\ Files/Logo/Header.svg -o static/images/header.svg",
    "svgo": "yarn run logo && yarn run header"
  }
}
